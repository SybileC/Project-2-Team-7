CREATE TABLE master (
id serial PRIMARY KEY,
name VARCHAR UNIQUE NOT NULL,
age INT NOT NULL,
nationality VARCHAR UNIQUE NOT NULL,
overall INT NOT NULL,
potential INT NOT NULL,
club VARCHAR UNIQUE NOT NULL,
value INT NOT NULL,
wage INT NOT NULL,
special VARCHAR UNIQUE NOT NULL,
preferred_foot VARCHAR UNIQUE NOT NULL,
position VARCHAR UNIQUE NOT NULL,
height INT UNIQUE NOT NULL,
weight INT NOT NULL
)

